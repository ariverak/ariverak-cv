{
  "autoPlay": false,
  "fullScreen": {
    "enable": true,
    "zIndex": 0
  },
  "detectRetina": false,
  "fpsLimit": 30,
  "interactivity": {
    "events": {
      "onClick": {
        "enable": false
      },
      "onHover": {
        "enable": false
      },
      "resize": {
        "enable": true
      }
    }
  },
  "particles": {
    "move": {
      "enable": true,
      "speed": 0.5,
      "random": false,
      "straight": false
    },
    "number": {
      "density": {
        "enable": true,
        "width": 800,
        "height": 800
      },
      "value": 8
    },
    "opacity": {
      "value": {
        "min": 0.2,
        "max": 0.4
      },
      "animation": {
        "enable": true,
        "speed": 0.5,
        "sync": false,
        "startValue": "random"
      }
    },
    "shape": {
      "type": "image",
      "options": {
        "image": [
          {
            "src": "/skills/react.svg",
            "height": 20,
            "width": 23
          },
          {
            "src": "/skills/javascript.png",
            "height": 20,
            "width": 20
          },
          {
            "src": "/skills/nodejs.svg",
            "height": 20,
            "width": 20
          }
        ]
      }
    },
    "size": {
      "value": {
        "min": 15,
        "max": 25
      },
      "animation": {
        "enable": true,
        "speed": 5,
        "sync": false,
        "startValue": "random"
      }
    },
    "links": {
      "enable": false
    }
  },
  "pauseOnBlur": true,
  "pauseOnOutsideViewport": true,
  "zLayers": 1
}